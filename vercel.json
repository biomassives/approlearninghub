{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "assets/**/*", 
      "use": "@vercel/static"
    },
    {Logs
      "src": "data/**/*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/modules/:context/:mod/",
      "dest": "/module.html?context=:context&mod=:mod"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem" 
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
